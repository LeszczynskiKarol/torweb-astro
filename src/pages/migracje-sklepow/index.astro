    description: "Średni sklep z integracjami",
    features: [
      "Do 10 000 produktów",
      "Pełna historia zamówień",
      "Integracje (płatności, kurierzy)",
      "BaseLinker/ERP sync",
      "Wielojęzyczność",
      "Custom atrybuty",
      "60 dni wsparcia"
    ],
    popular: true
  },
  {
    name: "Enterprise",
    price: "od 20 000",
    description: "Duży sklep, złożone wymagania",
    features: [
      "Nieograniczone produkty",
      "B2B + B2C",
      "Multi-store",
      "Custom development",
      "Migracja integracji custom",
      "Dedykowany PM",
      "SLA + 90 dni wsparcia"
    ],
    popular: false
  }
];

const platforms = [
  { name: "PrestaShop", direction: "from" },
  { name: "Magento", direction: "from" },
  { name: "Shoper", direction: "from" },
  { name: "Sky-Shop", direction: "from" },
  { name: "IdoSell", direction: "from" },
  { name: "OpenCart", direction: "from" },
  { name: "osCommerce", direction: "from" },
  { name: "Custom PHP", direction: "from" },
  { name: "WooCommerce", direction: "to" },
  { name: "Shopify", direction: "to" },
  { name: "Headless", direction: "to" }
];

const benefits = [
  { value: "0", label: "Przestojów w sprzedaży" },
  { value: "100%", label: "Danych zmigrowanych" },
  { value: "301", label: "Przekierowania dla SEO" },
  { value: "24h", label: "Monitoring po launchu" }
];

const seoChecklist = [
  "Mapowanie wszystkich URL (stare → nowe)",
  "Implementacja przekierowań 301",
  "Zachowanie meta title i description",
  "Migracja danych strukturalnych (schema)",
  "Aktualizacja sitemap.xml",
  "Zgłoszenie do Google Search Console",
