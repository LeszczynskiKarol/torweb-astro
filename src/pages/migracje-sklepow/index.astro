  },
  {
    icon: "seo",
    title: "SEO",
    items: ["URL structure", "Meta title/desc", "Przekierowania 301", "Schema markup", "Sitemap XML"]
  }
];

const process = [
  {
    phase: "Audyt & Analiza",
    duration: "2-5 dni",
    description: "Pełna analiza obecnego sklepu, danych i integracji",
    activities: ["Audyt techniczny", "Mapowanie danych", "Analiza integracji", "Plan migracji", "Risk assessment"]
  },
  {
    phase: "Przygotowanie",
    duration: "1-2 tygodnie",
    description: "Setup nowej platformy, skrypty migracyjne, środowisko testowe",
