    activities: ["Płatności", "Kurierzy", "BaseLinker/ERP", "Marketing", "Analityka"]
  },
  {
    phase: "UAT & Launch",
    duration: "3-5 dni",
    description: "Testy akceptacyjne, migracja finalna, przełączenie DNS",
    activities: ["Testy z klientem", "Finalna migracja", "Przekierowania 301", "DNS switch", "Monitoring"]
  }
];

const pricing = [
  {
    name: "Starter",
    price: "od 3 000",
    description: "Mały sklep, proste dane",
    features: [
      "Do 1 000 produktów",
      "Do 1 000 klientów",
